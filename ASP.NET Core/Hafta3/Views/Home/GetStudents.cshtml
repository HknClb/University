@model IEnumerable<Hafta3.Models.ModelsLists>

<!DOCTYPE html>
<html lang="tr">
<head>
    <title>Get Students Page</title>
    <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
</head>
<body>
    <p>Link Succesfuly Worked!</p>
    <table>
        <tr style="background-color: navajowhite; color: red; font-family: 'Times New Roman'; font-size: 14pt;">
            <th>Special ID</th>
            <th>School Number</th>
            <th>Name & Surname</th>
            <th>School Name</th>
            <th>Country</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr style="background-color: white; color: black; font-family: 'Times New Roman'; font-size: 12pt; text-align: center">
                <td>@item.ID</td>
                <td>@item.SchoolNumber</td>
                <td>@item.Name</td>
                <td>@item.SchoolName</td>
                <td>@item.Country</td>
            </tr>
        }
    </table>
</body>
</html>